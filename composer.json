{
    "name": "dnsimonenko/todo",
    "description": "Todo app",
    "license": "public",
    "type": "project",
    "autoload": {
        "psr-4": {
            "App\\": "src/",
            "TodoApp\\": "src/TodoApp"
        },
        "classmap": []
    },
    "require": {
        "php": ">=7.3",
        "sensio/framework-extra-bundle": "^5.1",
        "symfony/asset": "4.4.*",
        "symfony/console": "4.4.*",
        "symfony/dotenv": "4.4.*",
        "symfony/expression-language": "4.4.*",
        "symfony/flex": "^1.3",
        "symfony/form": "4.4.*",
        "symfony/framework-bundle": "4.4.*",
        "symfony/http-client": "4.4.*",
        "symfony/intl": "4.4.*",
        "symfony/mailer": "4.4.*",
        "symfony/monolog-bundle": "^3.1",
        "symfony/orm-pack": "*",
        "symfony/process": "4.4.*",
        "symfony/security-bundle": "4.4.*",
        "symfony/serializer-pack": "*",
        "symfony/translation": "4.4.*",
        "symfony/twig-pack": "*",
        "symfony/validator": "4.4.*",
        "symfony/web-link": "4.4.*",
        "symfony/yaml": "4.4.*",
        "marc-mabe/php-enum": "@stable",
        "ramsey/uuid": "@stable",
        "buttercup/protects": "dev-master",
        "predis/predis": "^1.1",
        "snc/redis-bundle": "^3.2"
    },
    "require-dev": {
        "phpunit/phpunit": "@stable",
        "behat/behat": "^3.6",
        "behat/mink": "^1.7",
        "behat/symfony2-extension": "^2.1",
        "behatch/contexts": "^3.3"
    },
    "config": {
        "bin-dir": "bin"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        }
    }
}
